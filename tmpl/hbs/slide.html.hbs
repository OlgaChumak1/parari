{{!
    Template for parari slide.
}}<!DOCTYPE html>
<!--
    This files is generated by {{pkg.name}}.
    At {{now}}
    For more detail, see {{pkg.homepage}}.
-->
<html>
<head>
<meta charset="utf-8"/>
<title>{{title}}</title>

<!-- Converted from {{favicon.filename}} -->
<link href="data:image/x-icon;base64,{{{favicon.data}}}" rel="icon" type="image/x-icon" />


    {{#each js}}<!-- Copied from {{filename}} -->
<script>{{{content}}}</script>
{{/each}}

{{#each css}}<!-- Copied from {{filename}} -->
<style>{{{content}}}</style>
{{/each}}



</head>
<body>
<div id="parari-slide-root">
{{{src}}}
</div>

<script>
parari.start('parari-slide-root', {
    vLock:false,
    hLock:true,
    layers: {
        {{#each layers}}{{name}}: [
            {{#each options}}{
            {{#each this}}    {{key}}:{{{val}}}{{#if @last}}{{else}},
            {{/if}}{{/each}}
            }{{#if @last}}{{else}},
            {{/if}}{{/each}}
        ]{{#if @last}}{{else}},
        {{/if}}{{/each}}
    }
});
</script>
</body>

</html>